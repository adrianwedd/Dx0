  "dashboard": {
    "title": "MAI DxO Metrics",
    "panels": [
      {
        "type": "timeseries",
        "title": "Panel Actions",
        "targets": [
          {"expr": "panel_actions_total", "legendFormat": "{{action_type}}"}
